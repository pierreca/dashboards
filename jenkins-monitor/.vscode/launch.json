{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "monitor",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/monitor.ts",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
            "preLaunchTask": "tsc",
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/lib"
		}
	]
}